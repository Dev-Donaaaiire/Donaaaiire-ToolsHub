{
    "version": 2,
    "builds": [
        {
            "src": "index.html",
            "use": "@vercel/static"
        },
        {
            "src": "tools/**/*.html",
            "use": "@vercel/static"
        },
        {
            "src": "tools/**/*.css",
            "use": "@vercel/static"
        },
        {
            "src": "tools/**/*.js",
            "use": "@vercel/static"
        },
        {
            "src": "tools/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/tools/(.*)",
            "dest": "/tools/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}